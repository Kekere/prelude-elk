                   rdfs:range :PhysicalImpact .


###  http://www.semanticweb.org/keren/ontologies/2022/6/untitled-ontology-4#hasProductEnumeration
:hasProductEnumeration rdf:type owl:ObjectProperty ;
                       rdfs:domain :Product ;
                       rdfs:range :ProductEnumeration .


###  http://www.semanticweb.org/keren/ontologies/2022/6/untitled-ontology-4#hasRemoteTypes
:hasRemoteTypes rdf:type owl:ObjectProperty ;
                owl:inverseOf :isRemoteType ;
                rdf:type owl:InverseFunctionalProperty .


###  http://www.semanticweb.org/keren/ontologies/2022/6/untitled-ontology-4#hasScenario
:hasScenario rdf:type owl:ObjectProperty ;
             rdfs:domain :Vulnerability ;
             rdfs:range :Scenario .


###  http://www.semanticweb.org/keren/ontologies/2022/6/untitled-ontology-4#hasScope
:hasScope rdf:type owl:ObjectProperty ;
          rdfs:domain :Impact ;
          rdfs:range :Scope .


###  http://www.semanticweb.org/keren/ontologies/2022/6/untitled-ontology-4#hasSectorOfInterest
:hasSectorOfInterest rdf:type owl:ObjectProperty ;
                     rdfs:domain :Vulnerability ;
                     rdfs:range :SectorOfInterest .


###  http://www.semanticweb.org/keren/ontologies/2022/6/untitled-ontology-4#id
:id rdf:type owl:ObjectProperty .


###  http://www.semanticweb.org/keren/ontologies/2022/6/untitled-ontology-4#isLimitedRemoteType
:isLimitedRemoteType rdf:type owl:ObjectProperty .


###  http://www.semanticweb.org/keren/ontologies/2022/6/untitled-ontology-4#isRemoteType
:isRemoteType rdf:type owl:ObjectProperty ,
                       owl:FunctionalProperty ;
              rdfs:domain :RemoteTypes ;
              rdfs:range :AttackTheater .


###  http://www.semanticweb.org/keren/ontologies/2022/6/untitled-ontology-4#neededPrivileges
:neededPrivileges rdf:type owl:ObjectProperty ;
                  rdfs:domain :Barrier ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty :barrierType ;
                                owl:someValuesFrom :Authentication
                              ] ;
                  rdfs:range :PrivilegeLevel .


###  http://www.semanticweb.org/keren/ontologies/2022/6/untitled-ontology-4#relatesToContext
:relatesToContext rdf:type owl:ObjectProperty ;
                  rdfs:domain :BoundaryProtections ;
                  rdfs:range <http://www.semanticweb.org/keren/ontologies/2021/7/vdo#Context> .


###  http://www.semanticweb.org/keren/ontologies/2022/6/untitled-ontology-4#requiresAttackTheater
:requiresAttackTheater rdf:type owl:ObjectProperty ;
                       rdfs:domain :Scenario ;
                       rdfs:range :AttackTheater .


###  http://www.semanticweb.org/keren/ontologies/2022/6/untitled-ontology-4#resultsInImpact
:resultsInImpact rdf:type owl:ObjectProperty ;
                 rdfs:domain :Action ;
                 rdfs:range :Impact .


###  http://www.semanticweb.org/keren/ontologies/2022/6/untitled-ontology-4#scheme
:scheme rdf:type owl:ObjectProperty .


###  http://www.semanticweb.org/keren/ontologies/2022/6/untitled-ontology-4#value
:value rdf:type owl:ObjectProperty .


