                       owl:inverseOf :isLimitedRemoteType .


###  http://www.semanticweb.org/keren/ontologies/2022/6/untitled-ontology-4#hasLocation
:hasLocation rdf:type owl:ObjectProperty ;
             rdfs:domain [ rdf:type owl:Restriction ;
                           owl:onProperty :hasLogicalImpact ;
                           owl:someValuesFrom :LogicalImpact
                         ] ;
             rdfs:range :Location ;
             owl:propertyDisjointWith :hasPhysicalImpact .


###  http://www.semanticweb.org/keren/ontologies/2022/6/untitled-ontology-4#hasLogicalImpact
:hasLogicalImpact rdf:type owl:ObjectProperty ;
