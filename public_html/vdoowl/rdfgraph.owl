#################################################################
#    Object Properties
#################################################################

###  http://www.semanticweb.org/keren/ontologies/2022/6/untitled-ontology-4#affectsContext
:affectsContext rdf:type owl:ObjectProperty ;
                rdfs:domain :Action ;
                rdfs:range <http://www.semanticweb.org/keren/ontologies/2021/7/vdo#Context> .


###  http://www.semanticweb.org/keren/ontologies/2022/6/untitled-ontology-4#affectsProduct
:affectsProduct rdf:type owl:ObjectProperty ;
                rdfs:domain :Scenario ;
                rdfs:range :Product .


###  http://www.semanticweb.org/keren/ontologies/2022/6/untitled-ontology-4#barrierType
:barrierType rdf:type owl:ObjectProperty ;
             rdfs:domain :Barrier ;
             rdfs:range [ rdf:type owl:Class ;
                          owl:unionOf ( :Authentication
                                        :BoundaryProtections
                                        :Obfuscation
                                        :State
                                      )
                        ] .


###  http://www.semanticweb.org/keren/ontologies/2022/6/untitled-ontology-4#blockedByBarrier
:blockedByBarrier rdf:type owl:ObjectProperty ;
                  rdfs:domain :Scenario ;
                  rdfs:range :Barrier .


###  http://www.semanticweb.org/keren/ontologies/2022/6/untitled-ontology-4#doesNotResultInImpact
:doesNotResultInImpact rdf:type owl:ObjectProperty ;
                       rdfs:domain :Action ;
                       rdfs:range :Impact .


###  http://www.semanticweb.org/keren/ontologies/2022/6/untitled-ontology-4#evidencedBySource
:evidencedBySource rdf:type owl:ObjectProperty ;
                   rdfs:domain :Scenario ;
                   rdfs:range :ResourceReference .


###  http://www.semanticweb.org/keren/ontologies/2022/6/untitled-ontology-4#gainedPrivileges
:gainedPrivileges rdf:type owl:ObjectProperty ;
                  rdfs:domain [ rdf:type owl:Restriction ;
                                owl:onProperty :hasLogicalImpact ;
                                owl:someValuesFrom :LogicalImpact
                              ] ;
                  rdfs:range :PrivilegeLevel .


###  http://www.semanticweb.org/keren/ontologies/2022/6/untitled-ontology-4#hasAction
:hasAction rdf:type owl:ObjectProperty ;
           rdfs:domain :Scenario ;
           rdfs:range :Action .


